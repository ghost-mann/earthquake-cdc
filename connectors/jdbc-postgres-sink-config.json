{
    "name": "earthquake-postgres-sink",
    "config": {
        "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
        "tasks.max": "1",
        "topics": "mariadb_server.earthquake.earthquake_data",
        "connection.url": "jdbc:postgresql://host.docker.internal:5432/earthquake",
        "connection.user": "postgres",
        "connection.password": "root",
        "insert.mode": "upsert",
        "table.name.format": "public.earthquake_data",
        "pk.mode": "record_key",
        "pk.fields": "eq_id",
        "auto.create": "false",
        "auto.evolve": "false",
        "delete.enabled": "true"
    }
}